  "scripts": {
    "dev": "npm run prepack -- --watch",
    "prepack": "tsup src/index.ts --format esm,cjs --dts --clean",
    "debug": "cd debug/vue2 && yarn serve",
    "publish:legacy": "npm publish --tag legacy",
    "test": "vitest run",
    "test:watch": "npm run prepack && vitest",
    "patch": "npm version patch --no-git-tag-version",
    "minor": "npm version minor --no-git-tag-version",
    "major": "npm version major --no-git-tag-version"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./index.d.ts",
  "files": [
    "dist",
    "index.d.ts"
  ],
